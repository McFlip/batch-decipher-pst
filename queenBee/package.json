{
  "name": "queen-bee",
  "version": "0.0.1",
  "engines": {
    "node": ">=15.8.0"
  },
  "description": "REST API between the client and the busyBee worker",
  "main": "index.js",
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' nyc mocha tests/index.test.ts --require ts-node/register --extensions ts,tsx --exit --bail",
    "start": "nodemon --watch index.ts --watch controllers --watch models --watch routes --watch util --watch types index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/McFlip/batch-decipher-pst.git"
  },
  "keywords": [
    "eDiscovery",
    "PST",
    "decrypt",
    "decipher"
  ],
  "author": "Grady Denton",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/McFlip/batch-decipher-pst/issues"
  },
  "homepage": "https://github.com/McFlip/batch-decipher-pst#readme",
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "workspace/*",
      "tests/*",
      "dist/*"
    ]
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "debug": "^2.6.9",
    "dockerode": "^3.3.2",
    "express": "^4.18.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.1",
    "@types/debug": "^4.1.7",
    "@types/dockerode": "^3.3.9",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^8.2.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.21",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
